<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_hr_feedback" model="ir.actions.act_window">
        <field name="name">Feedbacks</field>
        <field name="res_model">hr.feedback</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="action_hr_feedback_question" model="ir.actions.act_window">
        <field name="name">Questions du Feedback</field>
        <field name="res_model">hr.feedback.question</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_hr_feedback_root"
        name="Recrutement Feedback"
        sequence="10" />

    <menuitem id="menu_hr_feedback"
        name="Feedbacks"
        parent="menu_hr_feedback_root"
        action="action_hr_feedback"
        sequence="10" />

    <menuitem id="menu_hr_feedback_question"
        name="Questions"
        parent="menu_hr_feedback_root"
        action="action_hr_feedback_question"
        sequence="20" />

</odoo>